                                        <aura:if isTrue="{!index == 1}">
                                            <tr class="slds-line-height_reset slds-text-title_caps">            	
                                                <aura:iteration items="{!col}" var="firstCol">   
                                                    <th class="slds-is-resizable headClass1" scope="col">
                                                        <div class="slds-truncate" title="{!firstCol}">
                                                            {!firstCol}
                                                        </div>
                                                    </th>       
